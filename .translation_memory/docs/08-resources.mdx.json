    {
      "segment_id": "cc3c868d",
      "source_content": "# Resources <Badge text='ready' color='green'></Badge>",
      "source_content_hash": "b166189cf220b2bfc38cc5c2675bc5ff9f058e3e828ae2737b2163217c4983e6",
      "node_type": "heading",
      "translatable": true,
